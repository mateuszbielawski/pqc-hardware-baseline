// Clock: 27 MHz crystal oscillator
IO_LOC "clk" 4;
IO_PORT "clk" IO_TYPE=LVCMOS33;

// UART: Communication with Host (USB-to-Serial)
IO_LOC "uart_tx" 69;
IO_PORT "uart_tx" IO_TYPE=LVCMOS33;

IO_LOC "uart_rx" 70;
IO_PORT "uart_rx" IO_TYPE=LVCMOS33;

// Visual Feedback: Onboard LED
IO_LOC "led" 15;
IO_PORT "led" IO_TYPE=LVCMOS33;